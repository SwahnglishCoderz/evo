DB_DRIVER=mysql
DB_HOST=127.0.0.1
DB_NAME=mvc_basic
DB_USER=root
DB_PASSWORD=
DB_PREFIX=__mvc__
DB_PORT=3306

# DO NOT MODIFY
DB_COLLATE=utf8_unicode_ci
DB_CHARSET=utf8mb4
DB_ENGINE=InnoDB
DB_FORMAT=dynamic

SMTPDebug=0
SMTPAuth=true
SMTPSecure=tls
SMTPUsername=mailtrap_username
SMTPPassword=mailtrap_password
SMTPHost=smtp.mailtrap.io
Port=2525

GITHUB_EMAIL=USER_EMAIL
GITHUB_USERNAME=USERNAME
GITHUB_PASSWORD=PASSWORD
GITHUB_TOKEN=ACCESS_TOKEN
